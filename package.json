{
  "name": "@nuxt/design",
  "license": "CC-BY-ND-4.0",
  "version": "0.0.1",
  "exports": {
    "./dist/assets/*": "./dist/assets/*",
    "./dist/icons/*": "./dist/icons/*",
    "./dist/images/*": "./dist/images/*",
    "./dist/templates/*": "./dist/templates/*.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "yarn lint:all:eslint && yarn lint:all:prettier",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{json,vue,html}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepublishOnly": "yarn pinst --disable",
    "prerender": "jiti ./scripts/prerender",
    "postinstall": "husky install",
    "postpublish": "yarn pinst --enable"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@types/lodash.template": "^4.5.0",
    "@types/html-minifier": "^4.0.1",
    "critters": "^0.0.10",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^11.0.4",
    "html-minifier": "^4.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "lodash.template": "^4.5.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vite-plugin-windicss": "^1.2.4",
    "windicss": "^3.1.5"
  }
}
